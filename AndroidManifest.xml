            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <action android:name="pruss.mobi.superdim.LOAD_CUSTOM" />
            </intent-filter>
        </activity>
        <activity-alias android:name=".CreateShortcut0"
        	android:targetActivity=".AddShortcut"
        	android:label="SuperDim Night 1"
        	android:icon="@drawable/custom0"
        	>
        	<intent-filter>
        	<action android:name="android.intent.action.CREATE_SHORTCUT" />
            <category android:name="android.intent.category.DEFAULT" />
        	</intent-filter>
        </activity-alias>
        <activity-alias android:name=".CreateShortcut1"
        	android:targetActivity=".AddShortcut"
        	android:label="SuperDim Night 2"
        	android:icon="@drawable/custom1"
        	>
        	<intent-filter>
        	<action android:name="android.intent.action.CREATE_SHORTCUT" />
            <category android:name="android.intent.category.DEFAULT" />
        	</intent-filter>
        </activity-alias>
        <activity-alias android:name=".CreateShortcut2"
        	android:targetActivity=".AddShortcut"
        	android:label="SuperDim Night 3"
        	android:icon="@drawable/custom2"
        	>
        	<intent-filter>
        	<action android:name="android.intent.action.CREATE_SHORTCUT" />
            <category android:name="android.intent.category.DEFAULT" />
        	</intent-filter>
        </activity-alias>
        <activity-alias android:name=".CreateShortcut3"
        	android:targetActivity=".AddShortcut"
        	android:label="SuperDim Day 1"
        	android:icon="@drawable/custom3"
        	>
        	<intent-filter>
        	<action android:name="android.intent.action.CREATE_SHORTCUT" />
            <category android:name="android.intent.category.DEFAULT" />
        	</intent-filter>
        </activity-alias>
        <activity-alias android:name=".CreateShortcut4"
        	android:targetActivity=".AddShortcut"
        	android:label="SuperDim Day 2"
        	android:icon="@drawable/custom4"
        	>
        	<intent-filter>
        	<action android:name="android.intent.action.CREATE_SHORTCUT" />
            <category android:name="android.intent.category.DEFAULT" />
        	</intent-filter>
        </activity-alias>
        <service android:name=".ScreenOnListen">
        </service>

    </application>
     <uses-sdk android:minSdkVersion="6" />
     
    <uses-permission android:name="android.permission.WRITE_SETTINGS"/>
</manifest>